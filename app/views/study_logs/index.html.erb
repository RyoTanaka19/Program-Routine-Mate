<h1><font size= "7">投稿一覧画面</font></h1>
<br>
<% @study_logs.each do |study_log| %>
  <div class="mb-3">
    <p>学習内容: <%= study_log.content %></p>
    <p>学習時間: <%= study_log.hour %></p>
    <p> 感想など: <%= study_log.text %></p>
   </div>
<% end %>



<%= button_to '投稿画面へ遷移', new_study_log_path, method: :get, form: { data: { turbo_confirm: "投稿画面に遷移しますか？" } }, class: 'btn btn-primary' %> 
