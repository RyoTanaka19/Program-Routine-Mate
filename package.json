  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.12",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.23",
    "fullcalendar": "^6.1.17",
    "heatmap.js": "^2.0.5",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.1.4"
  }
}
